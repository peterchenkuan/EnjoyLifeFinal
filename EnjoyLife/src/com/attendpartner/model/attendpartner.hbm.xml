<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.attendpartner.model.AttendPartnerVO" table="attendpartner">
    	<composite-id>
    		<key-property name="eventNo" 
                          column="EVENTNO"/>
            <key-property name="partner" 
                          column="PARTNER"/>
    	</composite-id>
        <property name="partnerId" type="java.lang.String">
            <column name="PARTNERID" />
        </property>
        <property name="attend" type="java.lang.Integer">
            <column name="ATTEND" />
        </property>
    </class>
</hibernate-mapping>
